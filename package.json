{
  "name": "frontend",
  "version": "3.0.0",
  "private": true,
  "engineStrict": true,
  "engines": {
    "node": "v16.19.0"
  },
  "description": "A Vue.js & Spring Boot example project",
  "author": "CodeGuides GmbH <info@codeguides.de>",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --openssl-legacy-provider",
    "test:unit": "vue-cli-service test:unit --coverage",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "vue-cli-service test:unit && vue-cli-service test:e2e",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "1.20.1",
    "bootstrap": "^4.5.3",
    "bootstrap-vue": "^2.21.1",
    "connect-history-api-fallback": "2.0.0",
    "core-js": "^3.6.5",
     "cors": "2.8.5",
    "crypto-js": "^4.1.1",
    "dayspan-vuetify": "^0.4.0",
    "express": "4.18.2",
    "express-http-proxy": "^1.6.3",
    "http-proxy-middleware": "^2.0.6",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "request": "^2.88.2",
    "sass": "^1.43.2",
    "serve-static": "1.15.0",
    "vue": "^2.6.12",
    "vue-axios": "^2.1.4",
    "vue-cal": "^2.24.8",
    "vue-i18n": "^8.22.2",
    "vue-router": "^3.4.9",
    "vue2-editor": "^2.10.3",
    "vuejs-noty": "^0.1.3",
    "vuejs-quill": "^1.4.0",
    "vuetify": "~2.3.21",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@vue/babel-preset-app": "^4.5.11",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-nightwatch": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/test-utils": "^1.1.2",
    "ajv": "^6.12.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.6.12"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.vue$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|svg|png|jpg|ttf|woff|woff2)$": "jest-transform-stub",
      "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testMatch": [
      "**/tests/unit/**/*.spec.(js|jsx|ts|tsx)|**/__tests__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "coverageDirectory": "<rootDir>/tests/unit/coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,vue}",
      "!src/main.js",
      "!**/node_modules/**"
    ]
  }
}
